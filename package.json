{
  "name": "n8n-nodes-notion-advanced",
  "version": "1.0.0",
  "description": "Advanced n8n Notion node with comprehensive block type and formatting support for all 25+ block types (BETA)",
  "main": "dist/index.js",
  "scripts": {
    "build": "node dev-notes/build-for-install.js",
    "build-ts": "tsc && npm run copy-files",
    "copy-files": "copyfiles -u 1 \"nodes/**/*.{png,svg}\" dist/",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/NotionAdvanced/NotionAdvanced.node.js"
    ]
  },
  "keywords": [
    "n8n-community-node-package",
    "notion",
    "productivity",
    "api",
    "blocks",
    "rich-text",
    "database",
    "pages",
    "automation",
    "workflow",
    "integration",
    "notion-api",
    "block-types",
    "formatting"
  ],
  "license": "MIT",
  "homepage": "https://github.com/your-username/n8n-notion-advanced-node#readme",
  "author": {
    "name": "AZ-IT.US",
    "email": "azuniga6290@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AZ-IT-US/n8n-notion-advanced-node.git"
  },
  "engines": {
    "node": ">=16.10",
    "pnpm": ">=7.18"
  },
  "packageManager": "pnpm@7.18.0",
  "devDependencies": {
    "@types/node": "^18.16.16",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "copyfiles": "^2.4.1",
    "eslint": "^8.41.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
